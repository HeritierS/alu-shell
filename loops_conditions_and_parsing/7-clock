#!/usr/bin/env bash
# the scrix

hour=0
while [ $hour -le 12 ]; do
    echo "Hour: $hour"
    if [ $hour -eq 0 ]; then
        minute=1
    else
        minute=0
    fi
    while [ $minute -le 59 ]; do
        if [ $minute -lt 10 ]; then
            echo "0$minute"
        else
            echo $minute
        fi
        ((minute++))
    done
    ((hour++))
done | head -n 70
