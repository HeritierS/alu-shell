#!/usr/bin/env bash
# script for shell basics
hour=0
minute=1

while [ $hour -lt 12 ] || ([ $hour -eq 12 ] && [ $minute -lt 60 ])
do
    printf "%02d:%02d\n" $hour $minute

    if [ $minute -eq 59 ]
    then
        hour=$((hour + 1))
        minute=0
    else
        minute=$((minute + 1))
    fi
done
